const extractRightUntilEmpty = (keyword) => {
  let results = [];

  for (const cellAddress in sheet) {
    if (sheet[cellAddress]?.v === keyword) {
      const columnLetter = cellAddress.match(/[A-Z]+/)[0]; // Extract column letter
      const rowNumber = parseInt(cellAddress.match(/\d+/)[0]); // Extract row number

      // Move right and collect values until an empty cell is encountered
      let currentColumn = columnLetter;
      while (true) {
        currentColumn = String.fromCharCode(currentColumn.charCodeAt(0) + 1);
        const rightCellAddress = currentColumn + rowNumber;

        if (!sheet[rightCellAddress]?.v) break; // Stop if the right cell is empty

        results.push(sheet[rightCellAddress].v);
      }
      break; // Stop after finding the first occurrence of keyword
    }
  }

  return results.length > 0 ? results.join(", ") : "No valid data found";
};

// Example usage:
document.getElementById("test1").value = extractRightUntilEmpty("Deep Link");
