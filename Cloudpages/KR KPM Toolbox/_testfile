/** @format */

const findCellsContainingAmpLinks = () => {
  let results = [];

  for (const cellAddress in sheet) {
    if (sheet[cellAddress]?.v.includes("://amp/")) {
      results.push({ address: cellAddress, value: sheet[cellAddress].v });
    }
  }

  return results.length > 0
    ? results
    : [{ address: null, value: "No matching cells found" }];
};

// Example usage:
const ampCells = findCellsContainingAmpLinks();
console.log("Cells containing '://amp/':", ampCells);
