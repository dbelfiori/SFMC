<body>
    <h1>Automation Details</h1>
    %%[
    var @automations = RetrieveAllAutomations()
    function RetrieveAllAutomations() {
        var automations = Platform.Function.ParseJSON('%%=v(@automations)=%%')
        var rows = '';
        for (var i = 0; i < automations.length; i++) {
            rows += '<tr>';
            rows += '<td>' + automations[i].Name + '</td>';
            rows += '<td>' + automations[i].Description + '</td>';
            rows += '<td>' + automations[i].CustomerKey + '</td>';
            rows += '<td>' + automations[i].IsActive + '</td>';
            rows += '<td>' + automations[i].CreatedDate + '</td>';
            rows += '<td>' + automations[i].ModifiedDate + '</td>';
            rows += '<td>' + automations[i].Status + '</td>';
            rows += '<td>' + automations[i].ProgramID + '</td>';
            rows += '<td>' + automations[i].CategoryID + '</td>';
            rows += '<td>' + automations[i].LastRunTime + '</td>';
            rows += '<td>' + automations[i].ScheduledTime + '</td>';
            rows += '<td>' + automations[i].LastSaveDate + '</td>';
            rows += '<td>' + automations[i].ModifiedBy + '</td>';
            rows += '<td>' + automations[i].LastSavedBy + '</td>';
            rows += '<td>' + automations[i].CreatedBy + '</td>';
            rows += '<td>' + automations[i].AutomationType + '</td>';
            rows += '<td>' + automations[i].RecurrenceID + '</td>';
            rows += '</tr>';
        }
        return rows;
    }
    ]%%
    <table border="1">
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>CustomerKey</th>
            <th>IsActive</th>
            <th>CreatedDate</th>
            <th>ModifiedDate</th>
            <th>Status</th>
            <th>ProgramID</th>
            <th>CategoryID</th>
            <th>LastRunTime</th>
            <th>ScheduledTime</th>
            <th>LastSaveDate</th>
            <th>ModifiedBy</th>
            <th>LastSavedBy</th>
            <th>CreatedBy</th>
            <th>AutomationType</th>
            <th>RecurrenceID</th>
        </tr>
        %%=v(RetrieveAllAutomations())=%%
    </table>
</body>
</html>
